<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bhagi.enjoy">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".OptionActivity">
            <intent-filter>
                <action android:name="com.example.bhagi.enjoy.OptionActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".IntrstActivity">
            <intent-filter>
                <action android:name="com.example.bhagi.enjoy.IntrstActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".JocksActivity" />
        <activity android:name=".MtitleActivity" />
        <activity android:name=".MotivationActivity" />
        <activity android:name=".HistoryActivity" />
        <activity android:name=".HtitleActivity" />
        <activity android:name=".JtitleActivity" />
        <activity android:name=".KActivity" />
        <activity android:name=".KtitleActivity" />
        <activity android:name=".SActivity" />
        <activity android:name=".StitleActivity" />
        <activity android:name=".ItitleActivity"></activity>
    </application>

</manifest>